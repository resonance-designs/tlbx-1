{
  "name": "tlbx-1",
  "version": "0.1.11",
  "private": true,
  "description": "Project tooling for docs and scripting",
  "scripts": {
    "tlbx:dev": "cargo run",
    "tlbx:dev-opengl": "cargo run --features renderer-opengl",
    "tlbx:dev-vulkan": "cargo run --features renderer-vulkan",
    "tlbx:dev-bt": "set RUST_BACKTRACE=1&& cargo run",
    "tlbx:dev-bt-opengl": "set RUST_BACKTRACE=1&& cargo run --features renderer-opengl",
    "tlbx:dev-bt-vulkan": "set RUST_BACKTRACE=1&& cargo run --features renderer-vulkan",
    "tlbx:build": "node scripts/packaging/build.js",
    "tlbx:dev-docs": "node scripts/deploy-docs.js",
    "tlbx:dev-all": "node scripts/dev-build-run.js",
    "tlbx:where": "node scripts/find-bin.js",
    "version:sync": "node scripts/version-sync.js",
    "docs:install": "npm --prefix docs-site install",
    "docs:dev": "npm --prefix docs-site run start",
    "docs:build": "npm --prefix docs-site run build",
    "docs:serve": "npm --prefix docs-site run serve",
    "storybook": "npm --prefix docs-site run storybook",
    "storybook:build": "npm --prefix docs-site run storybook:build"
  }
}

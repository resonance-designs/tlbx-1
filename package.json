{
  "name": "grainrust",
  "version": "0.1.7",
  "private": true,
  "description": "Project tooling for docs and scripting",
  "scripts": {
    "grainrust:dev": "cargo run",
    "grainrust:dev-bt": "set RUST_BACKTRACE=1&& cargo run",
    "grainrust:build": "node scripts/packaging/build.js",
    "grainrust:dev-docs": "node scripts/deploy-docs.js",
    "grainrust:dev-all": "node scripts/dev-build-run.js",
    "grainrust:where": "node scripts/find-bin.js",
    "version:sync": "node scripts/version-sync.js",
    "docs:install": "npm --prefix docs-site install",
    "docs:dev": "npm --prefix docs-site run start",
    "docs:build": "npm --prefix docs-site run build",
    "docs:serve": "npm --prefix docs-site run serve",
    "storybook": "npm --prefix docs-site run storybook",
    "storybook:build": "npm --prefix docs-site run storybook:build"
  }
}
